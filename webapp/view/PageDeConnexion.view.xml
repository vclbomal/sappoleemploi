<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.newGestionDInventairePoleEmploi.controller.PageDeConnexion" xmlns:m="sap.m" xmlns:layout="sap.ui.layout">
    <m:Page showHeader="true" title="Authentification" showFooter="false" showNavButton="false">
        <m:content>
            <layout:BlockLayout>
                <layout:content>
                    <layout:BlockLayoutRow scrollable="false">
                        <layout:content>
                            <layout:BlockLayoutCell title="" titleAlignment="Begin" titleLevel="Auto" width="1">
                                <layout:content/>
                            </layout:BlockLayoutCell>
                            <layout:BlockLayoutCell title="" titleAlignment="Center" titleLevel="Auto" width="1">
                                <layout:content>
                                    <layout:BlockLayoutRow scrollable="false">
                                        <layout:content>
                                            <layout:BlockLayoutCell title="" titleAlignment="Begin" titleLevel="Auto" width="0">
                                                <layout:content>
                                                    <m:Image src="/webapp/resources/header-logo-pole-emploi-mono.png" height="auto" width="100%" densityAware="false"/>
                                                </layout:content>
                                            </layout:BlockLayoutCell>
                                        </layout:content>
                                    </layout:BlockLayoutRow>
                                    <layout:BlockLayoutRow scrollable="false">
                                        <layout:content>
                                            <layout:BlockLayoutCell title="" titleAlignment="Begin" titleLevel="Auto" width="0">
                                                <layout:content/>
                                            </layout:BlockLayoutCell>
                                        </layout:content>
                                    </layout:BlockLayoutRow>
                                    <m:Input placeholder="identifiant" type="Text" description="" showValueHelp="false" enabled="true" visible="true" width="100%" valueHelpOnly="false" maxLength="0"/>
                                    <m:Input placeholder="mot de passe" type="Password" showValueHelp="false" enabled="true" visible="true" width="100%" valueHelpOnly="false" maxLength="0"/>
                                    <layout:BlockLayoutRow scrollable="false">
                                        <layout:content>
                                            <layout:BlockLayoutCell title="" titleAlignment="Begin" titleLevel="Auto" width="0">
                                                <layout:content/>
                                            </layout:BlockLayoutCell>
                                        </layout:content>
                                    </layout:BlockLayoutRow>
                                    <m:Button text="" type="Accept" icon="sap-icon://unlocked" iconFirst="true" width="100%" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress3"/>
                                </layout:content>
                            </layout:BlockLayoutCell>
                            <layout:BlockLayoutCell title="" titleAlignment="End" titleLevel="Auto" width="1">
                                <layout:content/>
                            </layout:BlockLayoutCell>
                        </layout:content>
                    </layout:BlockLayoutRow>
                </layout:content>
            </layout:BlockLayout>
        </m:content>
        <m:footer>
            <m:Bar design="Auto">
                <m:contentLeft/>
                <m:contentMiddle/>
                <m:contentRight>
                    <m:Button text="Approve" type="Accept" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                    <m:Button text="Reject" type="Reject" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                    <m:Button text="" type="Default" icon="sap-icon://action" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                </m:contentRight>
            </m:Bar>
        </m:footer>
        <m:headerContent/>
        <m:subHeader/>
        <m:customHeader/>
    </m:Page>
</mvc:View>
